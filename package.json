{
  "name": "big-json-viewer",
  "version": "0.0.3",
  "description": "JavaScript Library to view big JSON structures.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "preversion": "tsc",
    "build-docs": "parcel build docs-src/index.html --out-dir docs --public-url ./",
    "watch-docs": "parcel docs-src/index.html --out-dir docs/tmp",
    "build-worker": "parcel build src/worker/json-parser.worker.ts --out-dir dist/worker --no-minify",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest",
    "publish-patch": "npm version patch && npm publish",
    "publish-minor": "npm version minor && npm publish"
  },
  "keywords": [
    "JSON",
    "JavaScript",
    "TypeScript",
    "big",
    "JSON",
    "viewer"
  ],
  "author": "Dominik Herbst",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhcode/big-json-viewer.git"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/jest": "^22.2.2",
    "babel-polyfill": "^6.26.0",
    "jasmine-core": "^2.99.1",
    "jest": "^22.4.3",
    "parcel-bundler": "^1.6.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
